<table class="table table-condensed">
  <thead>
    <tr>
      <th>Category</th>
      <th>Type</th>
      <th>Total</th>
      <th>Maximum</th>
      <th>Minimum</th>
      <th>Average</th>
      
    </tr>
</thead>
  <tbody>
    <% @try.each do |t| %>
      <tr>
        <td><%= t.c_name %></td>
        <td><%= t.c_type %></td>
        <td><%= t.count %></td>
        <td><%= t.max %></td>
        <td><%= t.min %></td>
        <td><%= t.avg %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>
